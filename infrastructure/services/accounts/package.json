{
    "name": "accounts",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "type": "module",
    "engines": {
        "npm": ">=6.0.0",
        "node": ">=20.0.0"
    },
    "scripts": {
        "seed": "nodemon prisma/seed.js",
        "start": "nodemon index.js",
        "migrate": "node manual-migration.mjs",
        "server": "concurrently -k npm:server:*",
        "server:rest": "json-server -w db.json -p 5000 -r routes.json -q",
        "server:graphql": "nodemon --ignore db.json -r dotenv/config ./index.js",
        "dev": "nodemon -r dotenv/config -e env,graphql,js ./index.js"
    },
    "dependencies": {
        "@apollo/datasource-rest": "^6.3.0",
        "@apollo/server": "^4.10.4",
        "@apollo/subgraph": "^2.8.0",
