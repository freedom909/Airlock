        "auth0": "^4.4.1",
        "axios": "^1.7.2",
        "dotenv": "^16.4.5",
        "express": "^4.19.2",
        "express-validator": "^7.1.0",
        "google-auth-library": "^9.11.0",
        "graphql": "^16.8.2",
        "graphql-middleware": "^6.1.35",
        "graphql-shield": "^7.6.5",
        "hashids": "^2.3.0",
        "ioredis": "^5.4.1",
        "jsonwebtoken": "^9.0.2",
        "mongodb": "^6.7.0",
        "mongoose": "^8.4.1",
        "nodemailer": "^6.9.13",
        "nodemon": "^3.1.3",
        "paseto": "^3.1.4",
